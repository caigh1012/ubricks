{
  "name": "ubricks",
  "version": "0.0.0",
  "description": "A javascript utility library",
  "exports": {
    ".": {
      "import": {
        "default": "./dist/index.es.js",
        "types": "./types/index.es.d.ts"
      },
      "require": {
        "default": "./dist/index.cjs.js",
        "types": "./types/index.cjs.d.ts"
      }
    }
  },
  "scripts": {
    "prepare": "husky || true",
    "preinstall": "npx only-allow pnpm",
    "docs:dev": "cross-env NODE_ENV=development vitepress dev docs",
    "docs:build": "cross-env NODE_ENV=production vitepress build docs",
    "docs:preview": "cross-env NODE_ENV=development vitepress preview docs",
    "build": "rollup --config rollup.config.ts --configPlugin typescript",
    "release": "release-it  -c .versionrc.js --ci --verbose",
    "release:dry": "release-it  -c .versionrc.js --ci --verbose --dry-run",
    "lint": "eslint **/*.{ts,js}",
    "lint:fix": "eslint **/*.{ts,js} --fix",
    "format": "prettier **/*.{ts,js,json,yaml,yml} --write .",
    "clean": "rimraf dist/ types/",
    "test": "jest",
    "lint-staged": "npx lint-staged"
  },
  "keywords": [
    "lodash",
    "lodash-es",
    "radash"
  ],
  "files": [
    "dist",
    "types"
  ],
  "author": "Kyler Tsai",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "bugs": {
    "url": "https://github.com/caigh1012/ubricks/issues"
  },
  "homepage": "https://caigh1012.github.io/ubricks",
  "repository": {
    "type": "git",
    "url": "https://github.com/caigh1012/ubricks.git"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/plugin-transform-runtime": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@jest/globals": "^29.7.0",
    "@release-it/conventional-changelog": "^8.0.1",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.45.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.2",
    "release-it": "^17.5.0",
    "rimraf": "^6.0.0",
    "rollup": "^4.18.1",
    "rollup-plugin-dts": "^6.1.1",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.3",
    "typescript": "5.1.6",
    "vitepress": "^1.3.0"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.24.7",
    "core-js": "^3.37.1"
  },
  "lint-staged": {
    "*.{ts,js,json,yaml,yml}": [
      "npm run format"
    ],
    "*.{ts,js}": [
      "npm run lint"
    ]
  }
}
